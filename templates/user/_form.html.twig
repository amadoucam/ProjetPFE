{{ form_start(form) }}
<div class="container"> 
   <div class="row">
    <div class="col-sm-12">{{ form_row(form.email, {'label': 'Email','attr': {'placeholder': 'exp: amdou@gmail.com'}}) }}</div>{#widget#}
    <div class="col-sm-12">{{ form_row(form.username, {'label': 'Username','attr': {'placeholder': 'exp: camara'}}) }}</div>
    <div class="col-sm-12"><br/>{{ form_row(form.nom, {'label': 'Nom'}) }}</div>
    <div class="col-sm-12"><br/>{{ form_row(form.prenom, {'label': 'Prenom','attr': {'placeholder': 'exp: amadou'}}) }}</div>
    <div class="col-sm-12">{{ form_row(form.datenaissance) }}</div>
    <div class="col-sm-12">{{ form_row(form.nationalite, {'label': 'Pays'}) }}</div>
    <div class="col-sm-12">{{ form_row(form.adresse, {'label': 'Adresse','attr': {'placeholder': 'exp: Votre adresse'}}) }}</div>{#widget#}
    <div class="col-sm-12">{{ form_row(form.avatar, {'label': 'Image'}) }}</div>
    <div class="col-sm-12"><br/>{{ form_row(form.postal_code, {'label': 'Code postal'}) }}</div>
    <div class="col-sm-12"><br/>{{ form_row(form.civility, {'label': 'Etat civil'}) }}</div>
    <div class="col-sm-12">{{ form_row(form.tel, {'label': 'Téléphone'}) }}</div>
    <div class="col-sm-12">{{ form_row(form.level, {'label': 'Niveau'}) }}</div>
   </div>
   
</div>
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}